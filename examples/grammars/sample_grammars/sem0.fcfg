% start S

S[SEM=<?vp(?subj)>] -> NP[SEM=?subj] VP[SEM=?vp]
VP[SEM=?v] -> V[SEM=?v]
NP[SEM=<cyril>] -> 'Cyril'
V[SEM=<\x.bark(x)>] -> 'barks'
