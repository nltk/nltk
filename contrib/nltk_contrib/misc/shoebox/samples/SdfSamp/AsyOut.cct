c asyout.cct Convert Assyrian for output
c Add paragraph styles to carry reversal

group(main)

'\id' > store(eat) use(id)

'\ct' > '\cp' nl dup

'\v ' > next
'\v' > '\vp' nl dup use(verse)

'\s' > '\sp' nl dup

'\mt' > '\mtp' nl dup

group(verse)

nl > next
' ' > nl '\vt ' use(main)

group(id)

'\' > endstore dup back(1) use(main)
